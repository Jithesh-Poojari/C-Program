#include <stdio.h>

#include <conio.h>

int main()

{

	int a, b, c, max;	clrscr();

	printf("Enter 3 numbers:");

	scanf("%d%d%d", &a, &b, &c);

	if (a > b)

		if (a > c)

			max = a;

		else

			max = c;

	else if (b > c)

		max = b;

	else

		max = c;

	printf("maximum is %d", max);

	getch();

	return 0;

}		
